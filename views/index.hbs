<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="style.css"/>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
  </head>
  <body>
    <div id="container">
      {{#each data.picture}}
        {{#each this}}
        <div class="patch">
          <p class="divPos">{{this.[pos]}}</p>
          {{#each this.[patch]}}
              {{#each this}}
                  <div style="background-color:{{this}};" class="grid"></div>
              {{/each}}
          {{/each}}
        </div>
        {{/each}}
      {{/each}}
    </div>
  <!-- <script>var data = {{data}}</script> -->
  <script src="/main.js"></script>

  </body>
</html>
